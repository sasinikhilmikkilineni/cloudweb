{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Sid":"AllowCloudFrontReadOnlyViaOAC",
      "Effect":"Allow",
      "Principal":{"Service":"cloudfront.amazonaws.com"},
      "Action":["s3:GetObject"],
      "Resource":"arn:aws:s3:::my-frontend-bucket/*",
      "Condition":{"StringEquals":{"AWS:SourceArn":"arn:aws:cloudfront:::distribution/"}}
    }
  ]
}
